{
  "name": "test-config",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/cjs/TestConfigManager.js",
  "module": "dist/TestConfigManager.js",
  "types": "dist/TestConfigManager.d.ts",
  "exports": {
    ".": {
      "import": "./dist/TestConfigManager.js",
      "require": "./dist/cjs/TestConfigManager.js"
    },
    "./CoverageConfigManager": {
      "import": "./dist/CoverageConfigManager.js",
      "require": "./dist/cjs/CoverageConfigManager.js"
    },
    "./TestRunner": {
      "import": "./dist/TestRunner.js",
      "require": "./dist/cjs/TestRunner.js"
    },
    "./TestReporter": {
      "import": "./dist/TestReporter.js",
      "require": "./dist/cjs/TestReporter.js"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/cjs/types.js"
    },
    "./DeploymentPipeline": {
      "import": "./dist/DeploymentPipeline.js",
      "require": "./dist/cjs/DeploymentPipeline.js"
    },
    "./deployment-targets": {
      "import": "./dist/deployment-targets.js",
      "require": "./dist/cjs/deployment-targets.js"
    },
    "./BlueGreenDeployment": {
      "import": "./dist/BlueGreenDeployment.js",
      "require": "./dist/cjs/BlueGreenDeployment.js"
    },
    "./blue-green-config": {
      "import": "./dist/blue-green-config.js",
      "require": "./dist/cjs/blue-green-config.js"
    },
    "./generate-nginx-config": {
      "import": "./dist/generate-nginx-config.js",
      "require": "./dist/cjs/generate-nginx-config.js"
    }
  },
  "bin": {
    "test-cli": "dist/TestCLI.js",
    "test-env": "dist/TestEnvironmentCLI.js",
    "test-runner": "dist/TestRunnerCLI.js",
    "coverage-config": "dist/CoverageConfigCLI.js",
    "deployment-pipeline": "dist/DeploymentPipelineCLI.js",
    "nginx-config": "dist/generate-nginx-config.js"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:env:start": "node dist/TestEnvironmentCLI.js start",
    "test:env:stop": "node dist/TestEnvironmentCLI.js stop",
    "test:env:status": "node dist/TestEnvironmentCLI.js status",
    "test:env:health": "node dist/TestEnvironmentCLI.js health",
    "test:env:cleanup": "node dist/TestEnvironmentCLI.js cleanup",
    "test:run:unit": "node dist/TestRunnerCLI.js unit",
    "test:run:integration": "node dist/TestRunnerCLI.js integration",
    "test:run:e2e": "node dist/TestRunnerCLI.js e2e",
    "test:run:all": "node dist/TestRunnerCLI.js all",
    "test:run:health": "node dist/TestRunnerCLI.js health",
    "coverage:show": "node dist/CoverageConfigCLI.js show",
    "coverage:report": "node dist/CoverageConfigCLI.js report",
    "coverage:badge": "node dist/CoverageConfigCLI.js badge",
    "coverage:collect": "node dist/CoverageConfigCLI.js collect",
    "coverage:validate": "node dist/CoverageConfigCLI.js validate",
    "coverage:jest-config": "node dist/CoverageConfigCLI.js jest-config",
    "deploy:validate": "node dist/DeploymentPipelineCLI.js validate",
    "deploy:validate:staging": "node dist/DeploymentPipelineCLI.js validate --target staging",
    "deploy:validate:production": "node dist/DeploymentPipelineCLI.js validate --target production",
    "deploy:build": "node dist/DeploymentPipelineCLI.js build",
    "deploy:staging": "node dist/DeploymentPipelineCLI.js deploy staging",
    "deploy:production": "node dist/DeploymentPipelineCLI.js deploy production",
    "deploy:staging:dry-run": "node dist/DeploymentPipelineCLI.js deploy staging --dry-run",
    "deploy:production:dry-run": "node dist/DeploymentPipelineCLI.js deploy production --dry-run",
    "deploy:rollback:staging": "node dist/DeploymentPipelineCLI.js rollback staging",
    "deploy:rollback:production": "node dist/DeploymentPipelineCLI.js rollback production",
    "deploy:history": "node dist/DeploymentPipelineCLI.js history",
    "deploy:status": "node dist/DeploymentPipelineCLI.js status",
    "nginx:generate": "node dist/generate-nginx-config.js generate",
    "nginx:generate-both": "node dist/generate-nginx-config.js generate-both",
    "nginx:switch": "node dist/generate-nginx-config.js switch",
    "nginx:validate": "node dist/generate-nginx-config.js validate",
    "nginx:reload": "node dist/generate-nginx-config.js reload"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "glob": "^11.0.3",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "open": "^9.1.0",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.5",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.15.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2"
  }
}
